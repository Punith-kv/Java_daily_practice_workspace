public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,5};
	    int max=a[0],min=a[0],i;
	    for(i=0;i<=a.length-1;i++){
	        if(a[i]>max){
	            max=a[i];
	        }
	    }
	    System.out.println("The maximum number in an array is "+max);
	    for(i=0;i<=a.length-1;i++){
	        if(a[i]<min){
	            min=a[i];
	        }
	    }
	    System.out.println("The minimum number in an array is "+min);
	    
		
	}
}
