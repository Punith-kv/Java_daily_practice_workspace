public class Main
{
	public static void main(String[] args) {
		
		int a[]={1,2,3,4,5};
		int i,j;
		boolean flag=false;
		for(i=0;i<a.length;i++)
		    for(j=i+1;j<a.length;j++){
		        if(a[i]==a[j]){
		            flag=true;
		        }
		    }
		if(flag==true){
		    System.out.println("The Duplicate Element is found");
		}
		else{
		    System.out.println("The Duplicate Element is not found");
		}
	}
}

//Another method using Hashset
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    HashSet <String>langs=new HashSet();
	    
		System.out.println(langs.add("java"));
		System.out.println(langs.add("python"));
		System.out.println(langs.add("java"));
	}
}
