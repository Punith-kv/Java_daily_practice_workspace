public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,5};
	    int sum=0,i;
	    for(i=0;i<=(a.length-1);i++){
	        sum=sum+a[i];
	    }
		System.out.println("Sum of the given array is "+sum);
	}
}


//Another method using Enchanced for loop

public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,5};
	    int sum=0;
	    for(int value:a){
	        sum=sum+value;
	    }
		System.out.println("Sum of the given array is "+sum);
	}
}

