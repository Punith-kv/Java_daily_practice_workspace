import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    System.out.println("Enter the given number:");
	    Scanner sc=new Scanner(System.in);
	    int num=sc.nextInt();
		int rev;
		int sum=0;
		while(num>0){
		    rev=num%10;
		    sum=sum+rev;
		    num=num/10;

		}

		System.out.println("the sum of digits of a given number is:"+sum);
		
	}
}
