
public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,5};
	    int i;
	    System.out.println("odd numbers of the given array is ");
	    for(i=0;i<=(a.length-1);i++){
	        if(a[i]%2==0){
	            System.out.println(a[i]);
	        }
	    }
	    System.out.println("even numbers of the given array is ");
	    for(i=0;i<=(a.length-1);i++){
	        if(a[i]%2!=0){
	            System.out.println(a[i]);
	        }
	    }
	
	}
}
