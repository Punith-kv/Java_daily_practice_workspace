//count of the numbers

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    System.out.println("Enter the given number:");
	    Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int count=0;
		while(num>0){
		    num=num/10;
		    count++;
		}
		System.out.println("the count of given number is:"+count);
		
	}
}
